<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="context1" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库相关配置-->
        <jdbcConnection connectionURL="jdbc:mysql://localhost:3306/fangguansuo?characterEncoding=utf8"
                        driverClass="com.mysql.jdbc.Driver"
                        password="root" userId="root"/>
        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
               NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- 生成封装类 -->
        <javaModelGenerator targetPackage="com.woniu.housemanager.pojo"
                            targetProject="E:\code\java\practiceontheclass\a0529vuerbac\src\main\java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 手动添加sqlMapGenerator,生成mapper的文件-->
        <sqlMapGenerator targetPackage="com.woniu.housemanager.mapper"
                         targetProject="E:\code\java\practiceontheclass\a0529vuerbac\src\main\java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <!--mapper文件对应的接口  -->
        <javaClientGenerator targetPackage="com.woniu.housemanager.mapper"
                             targetProject="E:\code\java\practiceontheclass\a0529vuerbac\src\main\java"
                             type="XMLMAPPER">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>
        <!-- 对那些表进行映射
        schema数据库 tableName表名
         -->
        <!--
      <table schema="shop" tableName="address">
          对列进行映射，表中的列和封装类属性一致时，可省
         <columnOverride column="???" property="???" />
      </table>  -->
        <!--        <table schema="relationshiptable" tableName="clazz"></table>-->
        <!--        <table schema="relationshiptable" tableName="student"></table>-->

        <table schema="fangguansuo" tableName="commercialhousingpresalepermitregistration"></table>

    </context>
</generatorConfiguration>